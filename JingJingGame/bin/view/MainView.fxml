<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainController">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resource/mainview.png" />
         </image></ImageView>
      <Pane layoutY="-14.0" prefHeight="720.0" prefWidth="1280.0">
         <children>
            <Label layoutX="19.0" layoutY="34.0" text="성장 게이지">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Label layoutX="408.0" layoutY="35.0" text="현재 상태: ">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Label fx:id="presentGrowth" layoutX="544.0" layoutY="26.0" text="Label" textFill="#f81eba">
               <font>
                  <Font size="40.0" />
               </font>
            </Label>
            <Button fx:id="changeGameView" layoutX="45.0" layoutY="323.0" mnemonicParsing="false" onAction="#changeToGameView" prefHeight="84.0" prefWidth="80.0" style="-fx-background-color: white;" text="&lt;">
               <font>
                  <Font size="35.0" />
               </font></Button>
            <Button fx:id="changeStore" layoutX="1151.0" layoutY="323.0" mnemonicParsing="false" onAction="#changeToStore" prefHeight="84.0" prefWidth="80.0" style="-fx-background-color: white;" text="&gt;">
               <font>
                  <Font size="35.0" />
               </font></Button>
            <Button fx:id="changeAD" layoutX="1139.0" layoutY="34.0" mnemonicParsing="false" onAction="#changeToAD" prefHeight="30.0" prefWidth="89.0" style="-fx-background-color: white;" text="광고보기" />
            <ProgressBar fx:id="experienceBar" layoutX="164.0" layoutY="36.0" prefHeight="29.0" prefWidth="200.0" progress="0.0" />
            <Label layoutX="714.0" layoutY="38.0" text="소지금:">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Label fx:id="moneyLabel" layoutX="839.0" layoutY="19.0">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
            <Button layoutX="19.0" layoutY="677.0" mnemonicParsing="false" onAction="#changeSceneToProlog" text="프롤로그(임시)" />
         </children>
      </Pane>
   </children>
</AnchorPane>
