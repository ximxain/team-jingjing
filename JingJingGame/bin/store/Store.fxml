<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="store.StoreController">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../picture/storeView.jpg" />
         </image>
      </ImageView>
      <Pane prefHeight="720.0" prefWidth="1280.0">
         <children>
            <Button fx:id="changeMainView" layoutX="45.0" layoutY="323.0" mnemonicParsing="false" onAction="#changeToMainView" prefHeight="84.0" prefWidth="80.0" style="-fx-background-color: white; -fx-border-color: black;" text="&lt;">
               <font>
                  <Font size="35.0" />
               </font></Button>
            <Button fx:id="showMyItem" layoutX="45.0" layoutY="637.0" mnemonicParsing="false" onAction="#myItem" prefHeight="38.0" prefWidth="108.0" style="-fx-background-color: white; -fx-border-color: black;" text="내 물건 보기" />
            <Button fx:id="storeBtn" layoutX="1108.0" layoutY="637.0" mnemonicParsing="false" onAction="#store" prefHeight="38.0" prefWidth="108.0" style="-fx-background-color: white; -fx-border-color: black;" text="상점" />
            <ProgressBar layoutX="1032.0" layoutY="97.0" prefHeight="38.0" prefWidth="211.0" progress="0.0" />
            <ProgressBar layoutX="1032.0" layoutY="27.0" prefHeight="38.0" prefWidth="211.0" progress="0.0" />
            <Label layoutX="909.0" layoutY="28.0" text="배고픔">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label layoutX="931.0" layoutY="98.0" text="아픔">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
