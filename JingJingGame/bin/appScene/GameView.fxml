<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appScene.GameController">
	<children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resource/gameview.png" />
         </image></ImageView>
		<Pane prefHeight="720.0" prefWidth="1280.0">
			<children>
            <Button fx:id="app" layoutX="309.0" layoutY="227.0" mnemonicParsing="false" prefHeight="8.0" prefWidth="27.0" text="Button" />
				<Label layoutX="560.0" layoutY="16.0" text="게임 종류">
					<font>
						<Font size="37.0" />
					</font>
				</Label>
				<ImageView fitHeight="145.0" fitWidth="147.0" layoutX="249.0" layoutY="161.0" onMouseClicked="#clickButtonGame" pickOnBounds="true">
					<image>
						<Image url="@../resource/a.jpg" />
					</image>
				</ImageView>
				<ImageView fitHeight="145.0" fitWidth="147.0" layoutX="599.0" layoutY="365.0" onMouseClicked="#clickCardGame" pickOnBounds="true">
					<image>
						<Image url="@../picture/CardGame.png" />
					</image>
				</ImageView>
				<ImageView fitHeight="145.0" fitWidth="147.0" layoutX="147.0" layoutY="360.0" onMouseClicked="#clickTickTacToe" pickOnBounds="true">
					<image>
						<Image url="@../resource/tietattoe.jpg" />
					</image>
				</ImageView>
				<ImageView fitHeight="145.0" fitWidth="147.0" layoutX="371.0" layoutY="360.0" onMouseClicked="#clickMath" pickOnBounds="true">
					<image>
						<Image url="@../resource/math%20icon.jpg" />
					</image>
				</ImageView>
				<ImageView fitHeight="145.0" fitWidth="147.0" layoutX="487.0" layoutY="166.0" onMouseClicked="#clickMusic" pickOnBounds="true">
					<image>
						<Image url="@../resource/music%20icon.jpg" />
					</image>
				</ImageView>
				<Label layoutX="263.0" layoutY="327.0" text="정예린을 이겨라 !" />
				<Label layoutX="628.0" layoutY="523.0" text="운빨카드게임" />
				<Label layoutX="174.0" layoutY="523.0" text="TIKTAKTOK !" />
				<Label layoutX="390.0" layoutY="523.0" prefHeight="19.0" prefWidth="128.0" text="수학퀴즈게임" />
				<Label layoutX="516.0" layoutY="323.0" text="징징리듬게임" />
            <Button fx:id="changeMainViewFromGame" layoutX="1186.0" layoutY="323.0" mnemonicParsing="false" onAction="#changeToMainViewFromGame" prefHeight="84.0" prefWidth="80.0" style="-fx-background-color: white;" text="&gt;">
               <font>
                  <Font size="35.0" />
               </font></Button>
			</children>
		</Pane>
	</children>
</AnchorPane>
