<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="QuizGame.MainLayOut">
   <children>
      <ImageView fitHeight="500.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resource/퀴즈화면.png" />
         </image>
      </ImageView>
      <Pane prefHeight="500.0" prefWidth="700.0">
         <children>
            <Button fx:id="Quiz4Wrong" layoutX="115.0" layoutY="150.0" mnemonicParsing="false" onAction="#ClickQuiz4Wrong" prefHeight="46.0" prefWidth="195.0" style="-fx-background-color: white; -fx-border-color: black;" text="wrong">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Button fx:id="Quiz4Correct" layoutX="115.0" layoutY="355.0" mnemonicParsing="false" onAction="#ClickQuiz4Correct" prefHeight="46.0" prefWidth="195.0" text="correct">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Label layoutX="109.0" layoutY="59.0" text="contents">
               <font>
                  <Font size="39.0" />
               </font></Label>
            <Button fx:id="Quiz4Wrong2" layoutX="115.0" layoutY="250.0" mnemonicParsing="false" onAction="#ClickQuiz4Wrong2" prefHeight="46.0" prefWidth="195.0" style="-fx-background-color: white; -fx-border-color: black;" text="wrong2">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </children>
      </Pane>
   </children>
</AnchorPane>
