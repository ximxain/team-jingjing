<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="store.StoreController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../resource/흰배경.png" />
         </image>
      </ImageView>
      <Pane prefHeight="700.0" prefWidth="400.0">
         <children>
            <Label layoutX="139.0" layoutY="36.0" text="STORE">
               <font>
                  <Font size="40.0" />
               </font>
            </Label>
            <Line endX="299.0" layoutX="100.0" layoutY="102.0" startX="-100.0" />
            <Button fx:id="foodBtn" layoutX="139.0" layoutY="187.0" mnemonicParsing="false" onAction="#foodpop" prefHeight="85.0" prefWidth="124.0" style="-fx-background-color: pink;" text="음식">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Button fx:id="MDCBtn" layoutX="139.0" layoutY="348.0" mnemonicParsing="false" onAction="#MDCpop" prefHeight="85.0" prefWidth="124.0" style="-fx-background-color: pink;" text="약">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <Button fx:id="clothesBtn" layoutX="138.0" layoutY="511.0" mnemonicParsing="false" onAction="#clothespop" prefHeight="85.0" prefWidth="124.0" style="-fx-background-color: pink;" text="옷">
               <font>
                  <Font size="20.0" />
               </font></Button>
         </children></Pane>
   </children>
</AnchorPane>
