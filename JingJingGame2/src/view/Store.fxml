<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.StoreController">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" />
      <Pane prefHeight="720.0" prefWidth="1280.0">
         <children>
            <Button fx:id="changeMainView" layoutX="45.0" layoutY="323.0" mnemonicParsing="false" onAction="#changeToMainView" prefHeight="84.0" prefWidth="80.0" text="&lt;">
               <font>
                  <Font size="35.0" />
               </font></Button>
            <Button fx:id="showMyItem" layoutX="45.0" layoutY="637.0" mnemonicParsing="false" onAction="#myItem" prefHeight="38.0" prefWidth="108.0" text="내 물건 보기" />
            <Button fx:id="storeBtn" layoutX="1108.0" layoutY="637.0" mnemonicParsing="false" onAction="#store" prefHeight="38.0" prefWidth="108.0" text="상점" />
         </children>
      </Pane>
   </children>
</AnchorPane>
